{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": "standard",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "semi": "off",
        "quotes": "off",
        "no-tabs": "off",
        "indent": "off",
        "handle-callback-err": "off",
        "n/handle-callback-err": "off"
    }
}
